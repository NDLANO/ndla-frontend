// Place your settings in this file to overwrite default and user settings.
{
    "prettier.singleQuote": true,
    "prettier.trailingComma": "all",
    "prettier.semi": true,
    "prettier.jsxBracketSameLine": true,
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/node_modules/**": true,
        "**/htdocs": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/htdocs": true
    }
}