{
  "name": "ndla-frontend",
  "version": "0.1.16",
  "description": "NDLA Frontend",
  "scripts": {
    "build": "razzle build",
    "postbuild": "cp build/assets.json build/public/assets.json",
    "test": "cross-env BABEL_ENV=test NODE_ENV=unittest jest",
    "test-ci": "cross-env BABEL_ENV=test NODE_ENV=unittest jest --maxWorkers=2",
    "tdd": "cross-env BABEL_ENV=test NODE_ENV=unittest jest --watch",
    "lint": "yarn run lint-prettier && eslint --ext .js --ext .jsx src server",
    "lint-prettier": "node prettier.js lint",
    "start": "razzle start",
    "start-without-ssr": "cross-env RAZZLE_DISABLE_SSR=true razzle start",
    "start-with-local-converter": "cross-env RAZZLE_LOCAL_ARTICLE_CONVERTER=true razzle start",
    "start-prod": "cross-env NODE_ENV=production node build/server | bunyan",
    "start-cypress-server": "cross-env NDLA_ENVIRONMENT=test node build/server",
    "start-cypress-server-local": "cross-env NDLA_ENVIRONMENT=prod razzle start",
    "now-build": "npm run build",
    "now-start": "cross-env NODE_ENV=production NOW=true node build/server",
    "pretest": "npm run lint",
    "prettier": "node prettier.js write",
    "cypress:open": "cypress open"
  },
  "jest": {
    "testRegex": "/__tests__/.*-test.(js|jsx)$",
    "setupFiles": ["./src/__tests__/_initTestEnv.js"]
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "devDependencies": {
    "chalk": "^2.3.0",
    "cypress": "^2.1.0",
    "enzyme": "^3.2.0",
    "eslint": "^4.18.1",
    "eslint-config-ndla": "^0.4.0",
    "eslint-plugin-graphql": "^2.1.0",
    "eslint-plugin-react": "^7.7.0",
    "flux-standard-action": "^2.0.0",
    "ndla-scripts": "^0.4.0",
    "nock": "^9.1.4",
    "prettier": "^1.9.1",
    "react-test-renderer": "^16.2.0",
    "redux-saga-test-plan": "^3.3.1",
    "sinon": "^4.1.2"
  },
  "dependencies": {
    "apollo-boost": "^0.1.4",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "btoa": "^1.1.2",
    "bunyan": "^1.8.12",
    "classnames": "2.2.5",
    "compression": "^1.7.1",
    "cross-env": "^5.1.1",
    "date-fns": "^1.29.0",
    "defined": "1.0.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "express": "^4.16.2",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "helmet": "^3.9.0",
    "history": "^4.6.3",
    "hoist-non-react-statics": "^2.3.0",
    "http-status": "^1.0.1",
    "isomorphic-unfetch": "^2.0.0",
    "jwt-decode": "^2.2.0",
    "mathjax": "^2.7.4",
    "ndla-article-scripts": "^0.3.4",
    "ndla-error-reporter": "^0.2.0",
    "ndla-i18n": "^0.2.1",
    "ndla-icons": "^0.3.0",
    "ndla-licenses": "^0.2.0",
    "ndla-tabs": "^0.4.2",
    "ndla-tracker": "^0.1.0",
    "ndla-ui": "^0.12.2",
    "ndla-util": "^0.2.0",
    "node-fetch": "^1.7.2",
    "normalize-css": "2.3.1",
    "parse-url": "^3.0.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-focus": "^3.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "postcss-reporter": "^5.0.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.1.0",
    "razzle": "^0.8.12",
    "razzle-config-utils": "^0.1.0",
    "react": "^16.3.2",
    "react-apollo": "^2.1.2",
    "react-bem-helper": "^1.4.1",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.1.3",
    "react-prop-types": "^0.4.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-tabs": "^2.1.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "serialize-javascript": "^1.4.0",
    "source-map-support": "^0.5.3",
