{
  "name": "ndla-frontend",
  "version": "0.1.16",
  "description": "NDLA Frontend",
  "scripts": {
    "pretest": "npm run lint",
    "clean": "rimraf ./htdocs/assets/*",
    "test": "cross-env NODE_ENV=unittest jest",
    "tdd": "cross-env NODE_ENV=unittest jest --watch",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js -p --bail",
    "start": "cross-env NODE_ENV=development DISABLE_SSR=true nodemon server --watch server",
    "start-with-ssr": "cross-env NODE_ENV=development nodemon server --watch server",
    "start-prod": "cross-env NODE_ENV=production node server",
    "lint": "eslint --ext .js --ext .jsx src server"
  },
  "jest": {
    "testRegex": "/__tests__/.*-test.(js|jsx)$",
    "setupFiles": [
      "./src/__tests__/_initTestEnv.js"
    ]
  },
  "engines": {
    "node": "~6.6.0",
    "npm": "~3.10.3"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-maybe-webpack": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-config-ndla": "^0.1.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "flux-standard-action": "^1.0.0",
    "jest": "^19.0.2",
    "json-loader": "0.5.4",
    "nock": "^9.0.9",
    "nodemon": "^1.11.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-focus": "1.0.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-reporter": "^3.0.0",
    "react-addons-test-utils": "^15.4.0",
    "redux-mock-store": "^1.2.1",
    "redux-saga-test-plan": "^2.2.1",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-manifest-plugin": "^1.1.0"
