{
  "builds": [
    {
      "src": "build/public/**",
      "use": "@vercel/static"
    },
    {
      "src": "build/server.mjs",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "build/public/static/$1"
    },
    {
      "source": "/favicon.ico",
      "destination": "build/public/favicon.ico"
    },
    {
      "source": "/(.*)",
      "destination": "/build/server.mjs"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=86400"
        }
      ]
    },
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=86400"
        }
      ]
    }
  ],
  "env": {
    "IS_VERCEL": "true",
    "NODE_ENV": "production"
  }
}
