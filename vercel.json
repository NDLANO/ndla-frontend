{
  "regions": ["dub1"],
  "installCommand": "yarn set version 4.9.2 --yarn-path && yarn install",
  "outputDirectory": "build",
  "buildCommand": "yarn build",
  "rewrites": [
    { "source": "/static/(.*)", "destination": "/public/static/$1" },
    { "source": "/(.*)", "destination": "/api" }
  ],
  "functions": {
    "api/index.mjs": {
      "includeFiles": "build/server.js"
    }
  },
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=86400"
        }
      ]
    }
  ],
  "env": {
    "IS_VERCEL": "true",
    "NODE_ENV": "production"
  }
}
